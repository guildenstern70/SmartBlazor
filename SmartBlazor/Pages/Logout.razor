@page "/logout"

@using SmartBlazor.Service
@inject ISessionService  SessionService
@inject NavigationManager NavigationManager

@code 
{
    protected override void OnInitialized()
    {
        SessionService.Logout();
        NavigationManager.NavigateTo("/");
    }
}
